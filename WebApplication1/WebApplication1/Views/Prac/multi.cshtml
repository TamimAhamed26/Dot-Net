
@model List<WebApplication1.Models.InfoAda>   

@{
    ViewBag.Title = "Multi";
}

<h2>View</h2>

    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
            </tr>
        }
    </table>
